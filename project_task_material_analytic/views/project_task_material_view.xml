<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_project_task_form_material" model="ir.ui.view">
        <field name="name">project.task.form.material</field>
        <field name="model">project.task</field>
        <field
            name="inherit_id"
            ref="project_task_material.view_project_task_form_material"
        />
        <field
            name="groups_id"
            eval="[(4, ref('analytic.group_analytic_accounting'))]"
        />
        <field name="arch" type="xml">
            <field name="quantity" position="after">
                <field name="analytic_line_id" readonly="1" />
            </field>
        </field>
    </record>
</odoo>
