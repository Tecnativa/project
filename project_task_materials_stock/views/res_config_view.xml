<openerp>
<data>

    <record id="view_config_settings" model="ir.ui.view">
        <field name="name">Project Application</field>
        <field name="model">project.config.settings</field>
        <field name="inherit_id" ref="project.view_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='module_project_issue_sheet']/../.." position="after">
                <group name="task_materials" string="Task Materials">
                    <label for="id" string="Project Task Materials"/>
                    <div name="project_task_materials">
                        <div groups="base.group_multi_company">
                            <label for="company_id"/>
                            <field name="company_id" class="oe_inline"/>
                        </div>
                        <div>
                            <label for="task_materials_analytic_journal_id"/>
                            <field name="task_materials_analytic_journal_id" class="oe_inline"/>
                        </div>
                    </div>
                </group>
            </xpath>
        </field>
    </record>

</data>
</openerp>
